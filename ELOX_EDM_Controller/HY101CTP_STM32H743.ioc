# CubeMX .ioc for HY101CTP (1024x600 RGB display + FT5x06 touch)

[ProjectManager]
ProjectName=HY101CTP_STM32H743
MainLocation=Src
ProjectType=STM32Cube
Toolchain=STM32CubeIDE

[Pinout]
# LTDC RGB
PI12=LTDC_R0
PI13=LTDC_R1
PI8=LTDC_R2
PI9=LTDC_R3
PI10=LTDC_R4
PC0=LTDC_R5
PB1=LTDC_R6
PG6=LTDC_R7
PE5=LTDC_G0
PE6=LTDC_G1
PI13=LTDC_G2
PG10=LTDC_G3
PI15=LTDC_G4
PI10=LTDC_G5
PI12=LTDC_G6
PI20=LTDC_G7
PE4=LTDC_B0
PG12=LTDC_B1
PD6=LTDC_B2
PG11=LTDC_B3
PI14=LTDC_B4
PI16=LTDC_B5
PI17=LTDC_B6
PI18=LTDC_B7

# LTDC Sync & Control
PG7=LTDC_CLK
PI8=LTDC_DISP
PI10=LTDC_HSYNC
PI19=LTDC_VSYNC
PF10=LTDC_DE
PA3=LTDC_PWM

# Touch Panel FT5x06
PF8=I2C1_SDA
PF7=I2C1_SCL
PF6=TP_RST
PD7=TP_INT

# Power
Pin1=5V
Pin2=5V
Pin3=GND
Pin4=3.3V
Pin12=GND
Pin36=GND

[LTDC]
Width=1024
Height=600
HSYNC=10
HBP=100
HFP=210
VSYNC=10
VBP=10
VFP=18
Polarity=ActiveLow
PixelClock=58.5MHz

[I2C1]
Mode=Standard
Speed=400kHz

[GPIO]
PI8=Output
PA3=PWM_Output
PF6=Output
PD7=Input

[TouchGFX]
Enable=True
LTDC=Enabled
I2C1=Enabled

# End of .ioc template
